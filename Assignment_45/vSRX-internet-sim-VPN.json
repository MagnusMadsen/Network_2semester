version 12.1X46-D30.2;
/* INET Router */
system {
    host-name INET;
    root-authentication {
        encrypted-password "$1$4TkbZDtp$6E8C6Bg7K6gnHR31XnJjl0"; ## Samme root-pass som de andre
    }
    services {
        ssh;
        telnet;
    }
}

interfaces {
    ge-0/0/0 {
        unit 0 { family inet { address 10.3.3.2/30; } }  # mod SRX1
    }
    ge-0/0/1 {
        unit 0 { family inet { address 10.4.4.1/30; } }  # mod SRX2
    }
}
routing-options {
    static {
        route 10.1.1.0/24 next-hop 10.3.3.1;
        route 10.2.2.0/24 next-hop 10.4.4.2;
    }
}